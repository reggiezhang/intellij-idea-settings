<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Path">
        <Item>
          <IdeaActionID>CopyPaths</IdeaActionID>
          <ShortCut>'⇧⌘C'</ShortCut>
          <Description>Copy Path</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>28</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Implementation(s)">
        <Item>
          <IdeaActionID>GotoImplementation</IdeaActionID>
          <ShortCut>'⌥⌘B'</ShortCut>
          <Description>Implementation(s)</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Module Settings">
        <Item>
          <IdeaActionID>OpenModuleSettings</IdeaActionID>
          <ShortCut>'⌘↓' or 'F4'</ShortCut>
          <Description>Open Module Settings</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>TODO</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>